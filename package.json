{
	"name": "@e280/science",
	"version": "0.1.0",
	"description": "ts/js testing library",
	"license": "MIT",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"type": "module",
	"main": "./x/index.js",
	"exports": {
		".": "./x/index.js"
	},
	"files": [
		"x",
		"s"
	],
	"scripts": {
		"build": "tsc",
		"build-watch": "tsc -w",
		"count": "find s -path '*/_archive' -prune -o -name '*.ts' -exec wc -l {} +",
		"test": "node x/example.test.js --verbose",
		"test-watch": "node --watch x/example.test.js --verbose",
		"start": "run-p build-watch test-watch"
	},
	"devDependencies": {
		"@types/node": "^24.3.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.9.2"
	},
	"keywords": [
		"testing",
		"scientific",
		"toolkit"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/e280/science.git"
	},
	"bugs": {
		"url": "https://github.com/e280/science/issues"
	},
	"homepage": "https://github.com/e280/science#readme"
}
